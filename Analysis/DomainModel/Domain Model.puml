@startuml
title Domain Model - Room Booking System
skinparam classAttributeIconSize 0

class Student {
  studentId : int
  name : string
  email : string
}

class Room {
  roomId : int
  roomNumber : string
  capacity : int
  building : string
}

class Booking {
  bookingId : int
  bookingDate : string
  startTime : string
  endTime : string
}

class CheckIn {
  checkInId : int
  checkInTime : string
}

class MaintenanceTicket {
  ticketId : int
  description : string
  priority : string
}

class Credential {
  username : string
  password : string
}

class BookingStatus {
  statusId : int
  statusName : string
}

class TicketStatus {
  statusId : int
  statusName : string
}

Student "1" -- "1" Credential : authenticates
Student "1" -- "0..*" Booking : makes
Student "1" -- "0..*" MaintenanceTicket : submits

Booking "1" -- "1" Room : reserves
Booking "1" -- "1" BookingStatus : has
Booking "0..1" -- "0..1" CheckIn : has

MaintenanceTicket "1" -- "1" Room : for
MaintenanceTicket "1" -- "1" TicketStatus : has
@enduml